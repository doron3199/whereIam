<?xml version='1.0'?>

<robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find my_robot)/urdf/my_robot.gazebo" />
  <link name="robot_footprint"></link>


  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <joint name="robot_footprint_joint2" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="cart_body" />
  </joint>

  <link name="cart_body">
    <pose> 0 0 0 0 0 0 </pose>
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.5" ixy="0" ixz="0"
          iyy="0.3" iyz="0"
          izz="0.4"
      />
    </inertial>
    <collision name='cart_collision'>
      <origin xyz="0 0 0.4" rpy=" 0 0 0"/> 
      <geometry>
        <box size="1 0.6 0.8"/>
      </geometry>
    </collision>

    <visual name='cart_visual_button'>
      <origin xyz="0 0 0.3" rpy=" 0 0 0"/>
      <geometry>
        <box size="1 0.6 0.05"/>
      </geometry>
      <origin xyz="-0.5 -0.25 0" rpy=" 0 0 0"/>
    </visual>

    <visual name='cart_visual_middle'>
      <origin xyz="0 0 0.5" rpy=" 0 0 0"/>
      <geometry>
        <box size="1 0.6 0.05"/>
      </geometry>
    </visual>

    <visual name='cart_visual_top'>
      <origin xyz="0 0 0.7" rpy=" 0 0 0"/>
      <geometry>
        <box size="1 0.6 0.05"/>
      </geometry>
    </visual>

    <visual name='cart_visual_fl'>
      <origin xyz="0.5 0.25 0.4" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.8"/>
      </geometry>
    </visual>

    <visual name='cart_visual_bl'>
      <origin xyz="-0.5 0.25 0.4" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.8"/>
      </geometry>
    </visual>

    <visual name='cart_visual_fr'>
      <origin xyz="0.5 -0.25 0.4" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.8"/>
      </geometry>
    </visual>

    <visual name='cart_visual_br'>
      <origin xyz="-0.5 -0.25 0.4" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.8"/>
      </geometry>
    </visual>

  </link>

  <link name='chassis'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.55" ixy="0" ixz="0"
          iyy="0.3" iyz="0"
          izz="0.025"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size="1.1 0.6 0.05"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="1.1 0.6 0.05"/>
      </geometry>
    </visual>

  </link>





  <link name='left_wheel'>

    <inertial>
      <mass value="0.2"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.05" ixy="0" ixz="0"
          iyy="0.025" iyz="0"
          izz="0.05"
      />
    </inertial>

    <collision name='left_whee_collision'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/> 
      <geometry>
        <cylinder  radius="0.1" length="0.05"/>
      </geometry>
      <surface>
          <friction>
            <ode>
              <mu>1</mu>
              <mu2>1</mu2>
              <slip1>0.5</slip1>
              <slip2>0</slip2>
            </ode>
          </friction>
        </surface>
    </collision>

    <visual name='left_wheel_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder  radius="0.1" length="0.05"/>
      </geometry>
    </visual>

  </link>

<link name='right_wheel'>

    <inertial>
      <mass value="0.2"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.05" ixy="0" ixz="0"
          iyy="0.025" iyz="0"
          izz="0.05"
      />
    </inertial>

    <collision name='right_wheel_collision'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/> 
      <geometry>
        <cylinder  radius="0.1" length="0.05"/>
      </geometry>
      <surface>
          <friction>
            <ode>
              <mu>1</mu>
              <mu2>1</mu2>
              <slip1>0.5</slip1>
              <slip2>0</slip2>
            </ode>
          </friction>
        </surface>
    </collision>

    <visual name='right_wheel_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder  radius="0.1" length="0.05"/>
      </geometry>
    </visual>

  </link>

<link name='back_left_wheel'>

    <inertial>
      <mass value="0.2"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.05" ixy="0" ixz="0"
          iyy="0.025" iyz="0"
          izz="0.05"
      />
    </inertial>

    <collision name='back_left_whee_collision'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/> 
      <geometry>
        <cylinder  radius="0.1" length="0.05"/>
      </geometry>
      <surface>
          <friction>
            <ode>
              <mu>1</mu>
              <mu2>1</mu2>
              <slip1>0.5</slip1>
              <slip2>0</slip2>
            </ode>
          </friction>
        </surface>
    </collision>

    <visual name='back_left_wheel_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder  radius="0.1" length="0.05"/>
      </geometry>
    </visual>

  </link>

  <joint type="continuous" name="left_wheel_hinge">
    <origin xyz="0.45 0.35 0" rpy="0 0 0"/>
    <child link="left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint type="continuous" name="right_wheel_hinge">
    <origin xyz="0.45 -0.35 0" rpy="0 0 0"/>
    <child link="right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint type="continuous" name="back_left_wheel_hinge">
    <origin xyz="-0.45 0.0 0" rpy="0 0 0"/>
    <child link="back_left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>


  <link name='camera'>

    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <box_inertia
      m="0.1" x="0.05" y="0.05" z="0.05"
      />
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>

    <collision name='camera_collision'>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box  size="0.05 0.05 0.05"/>
      </geometry>
    </collision>

    <visual name='camera_visual'>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box  size="0.05 0.05 0.05"/>
      </geometry>
    </visual>

  </link>

  <joint type="fixed" name="camera_joint">
    <origin xyz="0.55 0 0" rpy="0 0 0"/>
    <child link="camera"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>


<link name='hokuyo'>

    <inertial>
      <mass value="1e-5"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <box_inertia
      m="0.1" x="0.05" y="0.05" z="0.05"
      />
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>

    <collision name='hokuyo_collision'>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box  size="0.1 0.1 0.1"/>
      </geometry>
    </collision>

    <visual name='hokuyo_visual'>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot/meshes/hokuyo.dae"/>
      </geometry>
    </visual>

  </link>

  <joint type="fixed" name="hokuyo_joint">
    <origin xyz="0.5 0 .06" rpy="0 0 0"/>
    <child link="hokuyo"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>

</robot>
